{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "GridfinityLabelAutogen.res.schema.label",
    "title": "Label",
    "description": "The Official gridfinity_label_parametric.f3d file.",
    "type": "object",
    "properties": {
        "gla_id": {
            "description": "Document parameter whose comment specifies that this document is an official GLA Fusion 360 file.",
            "type": "string",
            "const": "gridfinity_label_parametric",
            "document_parameter_name": "gla_id"
        },
        "picto_inner_diameter": {
            "description": "",
            "minimum": 0.0,
            "exclusiveMinimum": 0.0,
            "document_parameter_name": "picto_inner_diameter",
            "type": "number",
            "default": 2.0,
            "unit": "mm"
        },
        "picto_outer_diameter": {
            "description": "",
            "exclusiveMinimum": 0.0,
            "document_parameter_name": "picto_outer_diameter",
            "type": "number",
            "default": 4.0,
            "unit": "mm"
        },
        "picto_inner_length": {
            "description": "",
            "exclusiveMinimum": 0.0,
            "document_parameter_name": "picto_inner_length",
            "type": "number",
            "default": 4.0,
            "unit": "mm"
        },
        "picto_outer_length": {
            "description": "",
            "exclusiveMinimum": 0.0,
            "document_parameter_name": "picto_outer_length",
            "type": "number",
            "default": 4.0,
            "unit": "mm"
        },
        "bin_span": {
            "description": "",
            "exclusiveMinimum": 0.0,
            "document_parameter_name": "bin_span",
            "type": "number",
            "default": 1.0,
            "unit": "unitless"
        },
        "label_text": {
            "document_parameter_name": "label_text",
            "type": "string",
            "default": "[label_text]",
            "unit": "unitless"
        }
    },
    "required": [
        "gla_id",
        "picto_inner_diameter",
        "picto_outer_diameter",
        "picto_inner_length",
        "picto_outer_length",
        "bin_span",
        "label_text"
    ]
}